extends layout

block content
  .page-header
    .container
        h3
          | Gallery
          small.pull-right.hidden-xs
            a(href='./') Home >
            a.active(href='#')  Gallery
        small.visible-xs
          a(href='./') Home
          a.active(href='#') Gallery
  .container.gallery
    .row
      .col-sm-3
        a.image-link(href='./includes/img/project-1.jpg')
          img.img-thumbnail.img-responsive(src='./includes/img/project-1.jpg', alt='')
      .col-sm-3
        a.image-link(href='./includes/img/project-2.jpg')
          img.img-thumbnail.img-responsive(src='./includes/img/project-2.jpg', alt='')
      .col-sm-3
        a.image-link(href='./includes/img/project-3.jpg')
          img.img-thumbnail.img-responsive(src='./includes/img/project-3.jpg', alt='')
      .col-sm-3
        a.image-link(href='./includes/img/project-4.jpg')
          img.img-thumbnail.img-responsive(src='./includes/img/project-4.jpg', alt='')

    .row
      - each itemURL in itemURLs
        p
          = itemURL

          //- a.image-link(href='./includes/img/project-4.jpg')
          //- img.img-thumbnail.img-responsive(src='', alt='')
          //- p= itemURL
